{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "compile_before_run",
      "command": "/usr/bin/gcc-11",
      "args": [
        "-fdiagnostics-color=always",
        "-pthread",
        "-g",
        "-Wall",
        "${workspaceFolder}/**.c",
        "${workspaceFolder}/**.h",
        "-o",
        "${fileDirname}/hw2"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
